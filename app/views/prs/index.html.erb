<!-- Container (About Jumbotron) -->
<div class="jumbotron text-center">
    <h1>CARAMELLA</h1> 
    <p>We specialize in blablabla</p> 
    <a class="btn btn-default">
        더보기
        <span class="glyphicon glyphicon-chevron-right"></span>
    </a>
</div>
<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>views</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @prs.each do |pr| %>
      <tr>
        <td><%= pr.title %></td>
        <td><%= pr.content %></td>
        <td><%= "#{pr.impressionist_count} views" %></td>
        <td><%= link_to 'Show', pr %></td>
        <td><%= link_to 'Edit', edit_pr_path(pr) %></td>
        <td><%= link_to 'Destroy', pr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pr', new_pr_path %>
